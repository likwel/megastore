    function parseCookies(cookieString) {
        return cookieString.split(';').reduce((cookies, cookie) => {
            const [name, value] = cookie.trim().split('=');
            cookies[name] = decodeURIComponent(value);
            return cookies;
        }, {});
    }

    function saveD(e){

        let vendor = document.cookie;

        const cookies = parseCookies(document.cookie);
        let id_vendor = JSON.parse(cookies.vendor).id;

        let data = e.getAttribute("data-j"); 
        let isFull = document.querySelector("#isFull").checked;
        let isNotFull = document.querySelector("#isNotFull").checked;

        let obj = {
            vendor_id : id_vendor,
            day : data,
            isFull: isFull,
            isNotFull : isNotFull,
            custom : {
                morning : {
                    start : document.querySelector("#morningStart").value,
                    end : document.querySelector("#morningEnd").value
                },
                afternoon : {
                    start : document.querySelector("#afternoonStart").value,
                    end : document.querySelector("#afternoonEnd").value
                }
            }
        }

        fetch("/set-horaire",{
            method: 'POST',
            headers: {
                'Content-Type': 'application/json',
            },
            body: JSON.stringify(obj), // Votre objet JSON √† envoyer
        }).then(resp=> resp.json())
        .then(l=>{
            console.log(l);

        })
