                            <form action="/save-user" class="collapse show" id="registerUser" method="post" name="registerUser">

                                <input type="hidden" value="client" name="types" id="types"/>
                                <div class="form-group"><label for="fullname">Votre nom complet</label>
                                    <div class="input-group mb-4">
                                        <div class="input-group-prepend"><span class="input-group-text"><span
                                                    class="fa-solid fa-file-signature"></span></span></div>
                                        <input class="form-control" id="fullname"
                                            placeholder="Ex. RALAMBOMANANA J. Pierre" type="text"
                                            aria-label="email adress" name="fullname">
                                    </div>
                                </div>
                                <div class="form-group"><label for="username">Votre nom d'utilisateur</label>
                                    <div class="input-group mb-4">
                                        <div class="input-group-prepend"><span class="input-group-text"><span
                                                    class="fa-solid fa-file-signature"></span></span></div>
                                        <input class="form-control" id="username" placeholder="Ex. r.jeanpierre"
                                            type="text" aria-label="email adress" name="username">
                                    </div>
                                </div>
                                <div class="form-group"><label for="email">Votre e-mail</label>
                                    <div class="input-group mb-4">
                                        <div class="input-group-prepend"><span class="input-group-text"><span
                                                    class="fas fa-envelope"></span></span></div><input
                                            class="form-control" id="email" placeholder="Ex. example@company.com"
                                            type="text" aria-label="email adress" name="email">
                                    </div>
                                </div>
                                <div class="form-group"><label for="telephone">Votre téléphone</label>
                                    <div class="input-group mb-4">
                                        <div class="input-group-prepend"><span class="input-group-text"><span
                                                    class="fas fa-phone"></span></span></div><input class="form-control"
                                            id="telephone" placeholder="+261" type="text" aria-label="email adress"
                                            name="telephone">
                                    </div>
                                </div>
                                <div class="form-group"><label for="address">Votre adresse</label>
                                    <div class="input-group mb-4">
                                        <div class="input-group-prepend"><span class="input-group-text"><span
                                                    class="fas fa-location-dot"></span></span></div><input
                                            class="form-control" id="address"
                                            placeholder="Ex. Lot 48 TER Manankasina Antananarivo 101" type="text"
                                            aria-label="email adress" name="address">
                                    </div>
                                </div>
                                <div class="form-group"><label for="ville">Ville</label>
                                    <div class="input-group mb-4">
                                        <div class="input-group-prepend"><span class="input-group-text"><span
                                                    class="fas fa-city"></span></span></div><input class="form-control"
                                            id="ville" placeholder="Ex. Antananarivo" type="text"
                                            aria-label="email adress" name="ville">
                                    </div>
                                </div>
                                <div class="form-group"><label for="country">Pays</label>
                                    <div class="input-group mb-4">
                                        <div class="input-group-prepend"><span class="input-group-text"><span
                                                    class="fas fa-globe"></span></span></div><input class="form-control"
                                            id="country" placeholder="Ex. Madagascar" type="text"
                                            aria-label="email adress" name="country">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="form-group"><label for="password">Mot de passe</label>
                                        <div class="input-group mb-4">
                                            <div class="input-group-prepend"><span class="input-group-text"><span
                                                        class="fas fa-unlock-alt"></span></span></div><input
                                                class="form-control" id="password" placeholder="Mot de passe"
                                                type="password" aria-label="Password" autocomplete="on" required name="password">
                                        </div>
                                    </div>
                                    <div class="form-group"><label for="password_confirm">Confirmer le mot de
                                            passe</label>
                                        <div class="input-group">
                                            <div class="input-group-prepend"><span class="input-group-text"><span
                                                        class="fas fa-unlock-alt"></span></span></div><input
                                                class="form-control" id="password_confirm"
                                                placeholder="Confirmer le mot de passe" type="password"
                                                aria-label="Password" autocomplete="on" required name="password_confirm">
                                        </div>
                                    </div>
                                    <div class="form-check mb-4"><input class="form-check-input" type="checkbox"
                                            id="terms" name="terms"> <label class="form-check-label" for="terms"><span
                                                class="small">J'accepte les <a class="text-secondary"
                                                    href="https://demo.themesberg.com/bootstrap/spaces/html/terms.html">termes
                                                    et les conditions</a></span></label></div>
                                </div>
                                <button type="submit" class="btn btn-block btn-primary" onsubmit="hello()">Créer le compte</button>
                            </form>

